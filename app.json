{
  "expo": {
    "name": "Practice-HINH",
    "slug": "Practice-HINH",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
          {
            "fonts": ["./assets/fonts/Crimson_Text/CrimsonText-Bold.ttf", "./assets/fonts/Crimson_Text/CrimsonText-Regular.ttf", "./assets/fonts/Crimson_Text/CrimsonText-Italic.ttf", "./assets/fonts/Gothic_A1/GothicA1-Bold.ttf", "./assets/fonts/Gothic_A1/GothicA1-Regular.ttf", "./assets/fonts/Gothic_A1/GothiA1-ExtraBold.ttf"]
          }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
